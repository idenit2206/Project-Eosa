<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eosa.admin.mapper.RegionMapper">

    <select id="selectRegion" resultType="RegionDTO">
        SELECT
            R.regionIdx,
            R.regionName,
            R.regionPrice
        FROM Region R
    </select>

</mapper>